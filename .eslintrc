{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "jsx-a11y",
    "prettier"
  ],
  "rules": {
    "react/prop-types": false,
    "react/jsx-props-no-spreading": [
      true,
      {
        "exceptions": "Modal"
      }
    ]
  }
}